<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="1">
  <ParamType>RESOURCEITEM_PARAM_ST</ParamType>
  <DataVersion>2</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>201</FormatVersion>
  <Fields>
    <Field Def="s32 idleSfx = -1">
      <DisplayName>Idle SFX</DisplayName>
      <Description>Particle effect to play before activation.</Description>
    </Field>
    <Field Def="s32 chaseSfx = -1">
      <DisplayName>Chase SFX</DisplayName>
      <Description>Particle effect to play while chasing the player.</Description>
    </Field>
    <Field Def="s32 getSfx = -1">
      <DisplayName>Get SFX</DisplayName>
      <Description>Particle effect to play when reaching the player.</Description>
    </Field>
    <Field Def="s32 getItemId">
      <DisplayName>Get Item ID</DisplayName>
      <Description>Goods item to grant when picked up.</Description>
    </Field>
    <Field Def="u8 ResourceItemLimit" />
    <Field Def="dummy8 pad1[3]" />
    <Field Def="f32 startSpeedMin = 5">
      <DisplayName>Minimum Starting Speed</DisplayName>
      <Description>Lower bound of the random starting speed when activated.</Description>
      <DisplayFormat>%.2f</DisplayFormat>
    </Field>
    <Field Def="f32 startSpeedMax = 7">
      <DisplayName>Maximum Starting Speed</DisplayName>
      <Description>Upper bound of the random starting speed when activated.</Description>
      <DisplayFormat>%.2f</DisplayFormat>
    </Field>
    <Field Def="f32 acceleration = 3">
      <DisplayName>Acceleration</DisplayName>
      <Description>Rate of speed increase over time.</Description>
      <DisplayFormat>%.2f</DisplayFormat>
    </Field>
    <Field Def="f32 topSpeed = 10">
      <DisplayName>Top Speed</DisplayName>
      <Description>Top speed the resource can accelerate to.</Description>
      <DisplayFormat>%.2f</DisplayFormat>
    </Field>
    <Field Def="f32 forceGetTime = 5">
      <DisplayName>Force Get Time (s)</DisplayName>
      <Description>Time before the resource is automatically given to the player.</Description>
      <DisplayFormat>%.2f</DisplayFormat>
    </Field>
    <Field Def="dummy8 pad2[12]" />
  </Fields>
</PARAMDEF>